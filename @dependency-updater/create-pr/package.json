{
  "name": "@dependency-updater/create-pr",
  "version": "1.0.0",
  "description": "Update a file on github and create a pull request for it",
  "main": "distribution/index.js",
  "scripts": {
    "clean": "rm -rf distribution",
    "prebuild": "npm run clean",
    "build": "babel source/ --out-dir distribution/",
    "build:watch": "npm run build -- --watch",
    "test": "ava",
    "test:watch": "npm run test -- --watch"
  },
  "ava": {
    "babel": "inherit",
    "files": [
      "source/**/*.spec.js"
    ],
    "require": [
      "babel-register"
    ],
    "source": [
      "source/**/*.js",
      "!source/**/*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "6"
          }
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nerdlabs/dependency-updater.git"
  },
  "keywords": [
    "github",
    "file"
  ],
  "author": {
    "name": "nerdlabs",
    "email": "nerds@nerdlabs.it"
  },
  "contributors": [
    {
      "name": "Mario Nebl",
      "email": "hello@herebecode.com"
    },
    {
      "name": "Bjoern Brauer",
      "email": "zaubernerd@zaubernerd.de"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nerdlabs/dependency-updater/issues"
  },
  "homepage": "https://github.com/nerdlabs/dependency-updater/tree/master/%40dependency-updater/create-pr#readme",
  "devDependencies": {
    "ava": "0.16.0",
    "babel-cli": "^6.14.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.14.0",
    "sinon": "^1.17.6",
    "sinon-stub-promise": "^3.0.1"
  },
  "dependencies": {
    "github": "^11.0.0"
  }
}