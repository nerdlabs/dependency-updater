{
  "name": "@dependency-updater/create-pr",
  "version": "1.0.0",
  "description": "Update a file on github and create a pull request for it",
  "main": "distribution/index.js",
  "scripts": {
    "start": "npm run test -- --watch",
    "build": "babel source/ --out-dir distribution/",
    "pretest": "xo source/",
    "test": "ava"
  },
  "ava": {
    "babel": "inherit",
    "files": [
      "source/**/*.spec.js"
    ],
    "require": [
      "babel-register"
    ],
    "source": [
      "source/**/*.js"
    ]
  },
  "babel": {
    "presets": [
      ["env", {
        "targets": {
          "node": "6"
        }
      }]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nerdlabs/create-github-pull-request.git"
  },
  "keywords": [
    "github",
    "file"
  ],
  "author": {
    "name": "nerdlabs",
    "email": "nerds@nerdlabs.it"
  },
  "contributors": [
    {
      "name": "Mario Nebl",
      "email": "hello@herebecode.com"
    },
    {
      "name": "Bjoern Brauer",
      "email": "zaubernerd@zaubernerd.de"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nerdlabs/create-github-pull-request/issues"
  },
  "homepage": "https://github.com/nerdlabs/create-github-pull-request#readme",
  "devDependencies": {
    "ava": "0.16.0",
    "babel-cli": "^6.14.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "sinon": "^1.17.6",
    "sinon-stub-promise": "^3.0.1",
    "xo": "0.16.0"
  },
  "dependencies": {
    "github": "^3.1.0"
  }
}
