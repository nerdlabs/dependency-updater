declare module "github" {
	declare type RepoGetResponse = {
		default_branch: string;
	};

	declare type RepoContentResponse = {
		content: string;
		encoding: buffer$Encoding;
	};

	declare type RepoShaResponse = {
		sha: string;
	};

	declare type GitdataTreeResponse = {
		tree: Array<{path: string, sha: string}>;
	};

	declare export default class GitHub {
		authenticate(): void;
		repos: {
			get(): Promise<RepoGetResponse>;
			getContent(): Promise<RepoContentResponse>;
			getShaOfCommitRef(): Promise<RepoShaResponse>;
			updateFile(): void;
		};
		gitdata: {
			getTree(): Promise<GitdataTreeResponse>;
			createReference(): void;
		};
		pullRequests: {
			create(): void;
		};
	}
}
